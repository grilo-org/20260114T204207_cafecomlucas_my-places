    "leaflet": "^1.9.4",
    "next": "15.5.9",
    "next-pwa": "^5.6.0",
    "next-seo": "^7.0.1",
    "next-sitemap": "^4.2.3",
    "nextjs-progressbar": "^0.0.16",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-leaflet": "^5.0.0",
    "rxjs": "^7.8.2"
